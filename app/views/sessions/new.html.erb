<div class="container">
    <div class="page-header clearfix">

      <h1 class="page-title">Efetue seu login</h1>

    </div>

  <div class="two-third">

    <%= form_tag sessions_path do |f| %>

        <div class="form">

          <p class="input-block">
            <%=label_tag :email, "Email"%>
            <%=text_field_tag :email, params[:email]%>
          </p>

          <p class="input-block">
            <%=label_tag :password, "Senha"%>
            <%=password_field_tag :password%>
          </p>

          <div class="clear"></div>

          <%= submit_tag "Login" %>

        </div><!-- end #comment-form -->


    <% end %>

  </div>
</div>